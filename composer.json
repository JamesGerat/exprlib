{
    "name": "thesoftwarefarm/exprlib",
    "keywords": ["math", "expressions"],
    "homepage": "https://github.com/thesoftwarefarm/exprlib",
    "description": "An expression parser in PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "smassey",
            "homepage": "http://codehackit.blogspot.fr/"
        },{
            "name": "Community contributors",
            "homepage": "https://github.com/thesoftwarefarm/exprlib"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "autoload":     {
        "psr-0": {
            "exprlib": "src/"
        }
    },
    "require-dev": {
        "mageekguy/atoum": "master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
